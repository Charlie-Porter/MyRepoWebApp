@page
@model MyRepoWebApp.Pages.Folders.IndexModel

@{
    ViewData["Title"] = "Index";
}


<h1>Folders</h1>

<p>
    <a class="btn btn-secondary" asp-page="Create">Create New Folder</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FolderModel[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FolderModel[0].owner)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FolderModel[0].UpdateDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.FolderModel) {
        <tr>
            <td>
               <img src="data:image/png;base64," />
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.owner)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdateDate)
            </td>
            <td>
                <a class="btn btn-outline-light" asp-page="./Uploads/Uploads" asp-route-id="@item.ID">Uploads</a>
                <a class="btn btn-outline-light" asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
